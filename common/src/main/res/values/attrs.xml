<?xml version="1.0" encoding="utf-8"?>
<resources>


    <!--easyrecelview的圆形加载框-->
    <declare-styleable name="CircleProgress">
        <attr name="color1" format="reference|color" />
        <attr name="color2" format="reference|color" />
        <attr name="color3" format="reference|color" />
        <attr name="autoStart" format="boolean" />
    </declare-styleable>

    <!--侧滑菜单-->
    <declare-styleable name="SwipeMenuLayout">
        <attr name="swipeEnable" format="boolean"/>
        <attr name="ios" format="boolean"/>
        <attr name="leftSwipe" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="MarqueeView">
        <attr name="marqueeInterval" format="integer" />
        <attr name="marqueeAnimDuration" format="integer" />
        <attr name="marqueeAnimIn" format="reference" />
        <attr name="marqueeAnimOut" format="reference" />
    </declare-styleable>

    <!--仿QQ微信动态的九空格图片展示-->
    <declare-styleable name="NineGridView">
        <attr name="singleImgSize" format="dimension" />
        <attr name="doubleImgSize" format="dimension" />
        <attr name="imgGap" format="dimension" />
        <attr name="maxSize" format="integer" />
        <attr name="showStyle" format="enum">
            <enum name="grid" value="0" />
            <enum name="fill" value="1" />
        </attr>
    </declare-styleable>

</resources>