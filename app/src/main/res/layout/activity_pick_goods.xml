<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_pick_goods"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.administrator.mifei.activity.PickGoods">

    <include
        layout="@layout/title_back_view">
    </include>

    <ListView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/listViewPick"
        android:layout_weight="1">
    </ListView>

    <com.example.administrator.mifei.view.Total_Money
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </com.example.administrator.mifei.view.Total_Money>
</LinearLayout>
