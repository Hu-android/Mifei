<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:hjl="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_mine_comprehensive"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.administrator.mifei.activity.MineComprehensive">

    <include
        layout="@layout/title_back_view">
    </include>

    <com.example.administrator.mifei.view.txt_img_item
        android:id="@+id/btn_receive_address"
        android:layout_marginTop="@dimen/Android_25dp"
        android:layout_width="match_parent"
        android:layout_height="@dimen/Android_44dp"
        hjl:ti_item_txt="收货地址">
    </com.example.administrator.mifei.view.txt_img_item>
    <com.example.administrator.mifei.view.txt_img_item
        android:id="@+id/btn_wechat_binding"
        android:layout_width="match_parent"
        android:layout_height="@dimen/Android_44dp"
        hjl:ti_item_txt="微信绑定">
    </com.example.administrator.mifei.view.txt_img_item>
    <com.example.administrator.mifei.view.txt_img_item
        android:id="@+id/btn_problem_update"
        android:layout_width="match_parent"
        android:layout_height="@dimen/Android_44dp"
        hjl:ti_item_txt="问题反馈">
    </com.example.administrator.mifei.view.txt_img_item>


</LinearLayout>
